<%= render 'admin/shared/gift_option_tabs', :current => "Gift Choices" %>

<h2><%= t("editing_gift_choice")%></h2>

<%= error_messages_for :gift_choice %>

<% form_for object, :url => object_url, :html => { :method => :put, :multipart => true } do |f| %>
  <%= render :partial => "form", :locals => {:f => f} %>
  <p class="form-buttons">
    <%= render :partial => 'admin/shared/edit_resource_links' %>
  </p>
<% end %>
